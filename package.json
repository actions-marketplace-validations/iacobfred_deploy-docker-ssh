  "dependencies": {
    "@actions/core": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@vercel/ncc": "^0.33.1",
    "husky": "^8.0.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.0.4",
    "typescript": "^4.5.4"
  }
